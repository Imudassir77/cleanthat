{
    "syntax_version": "2",
    "meta": {
        "labels": [
            "cleanthat"
        ],
        "clean_pull_requests": true,
        "clean_main_branch": true
    },
    "source_code": {
        "includes": [
            "regex:.*\\.java"
        ],
        "excludes": [
            "regex:.*/generated/.*"
        ],
        "encoding": "UTF-8"
    },
    "languages": [
        {
            "language": "java",
            "language_version": "11",
            "processors": [
                {
                    "engine": "eclipse_formatter",
                    "parameters": {
                        "url": "classpath:config/eclipse_java_code_formatter.xml"
                    }
                }
            ]
        }
    ]
}